{
  "compilerOptions": {
    "allowJs": true,
    "declaration": true,
    "declarationMap": true,
    "jsx": "react",
    "target": "es5",
    "outDir": "./build",
    "declarationDir": "./build",
    "lib": ["es2016", "dom"],
    "esModuleInterop": true,
    "skipLibCheck": true,
    "module": "commonjs",
    "moduleResolution": "node"
  },
  "include": ["./src/**/*"]
}
